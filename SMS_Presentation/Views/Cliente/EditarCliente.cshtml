@using Presentation.Extensions
@using EntitiesServices.Work_Classes;
@model PlatMensagem_Solution.ViewModels.ClienteViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div id="containerModalDesc">
        <div class="modal fade" id="modalDesc" tabindex="-1" role="dialog" aria-labelledby="modalDesc" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="modalDesc">Mensagem Enviada</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div id="modalDescBody" class="modal-body">

                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    @using (Html.BeginForm("EnviarSmsCliente", "Cliente", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal" }))
    { 
            <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Enviar SMS</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                      @Html.Hidden("id", null, new { id = "idClie" })
                      Mensagem
                      @Html.TextBox("mensagem", null, new { @style = "height: 200px;", @class = "form-control" })
                  </div>
                  <div class="modal-footer">
                      @Html.ButtonActionSubmit("Enviar")
                    <button type="button" class="btn btn-sm btn-warning" data-dismiss="modal">Cancelar</button>
                  </div>
                </div>
              </div>
            </div>
    }

    <div class="page-title">
        <h3 class="title">Contatos</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaCliente", "Cliente")">Contatos</a>
            </li>
            <li class="active">
                <strong>Contatos - Edição</strong>
            </li>
        </ol>
        <br />
    </div>
