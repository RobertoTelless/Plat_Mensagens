@using Presentation.Extensions
@using EntitiesServices.Work_Classes;
@model PlatMensagem_Solution.ViewModels.AssinanteViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div id="containerModalDesc">
        <div class="modal fade" id="modalDesc" tabindex="-1" role="dialog" aria-labelledby="modalDesc" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="modalDesc">Mensagem Enviada</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div id="modalDescBody" class="modal-body">

                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    @using (Html.BeginForm("EnviarSmsAssinante", "Assinante", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal" }))
    { 
            <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Enviar SMS</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                      @Html.Hidden("id", null, new { id = "idClie" })
                      Mensagem
                      @Html.TextBox("mensagem", null, new { @style = "height: 200px;", @class = "form-control" })
                  </div>
                  <div class="modal-footer">
                      @Html.ButtonActionSubmit("Enviar")
                    <button type="button" class="btn btn-sm btn-warning" data-dismiss="modal">Cancelar</button>
                  </div>
                </div>
              </div>
            </div>
    }

    <div class="page-title">
        <h3 class="title">Assinantes</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaAssinante", "Assinante")">Assinantes</a>
            </li>
            <li class="active">
                <strong>Assinantes - Consulta</strong>
            </li>
        </ol>
        <br />
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Assinantes - Consulta</h5>
                </div>
                <div class="ibox-content">
                    @Html.AntiForgeryToken()
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                        </div>
                    }
                    <div class="form-group">
                        @Html.ButtonLinkIndex(text: "Voltar", action: "VoltarBaseAssinante", controller: "Assinante")
                        @Html.ButtonLinkIndex(text: "Dashboard", controller: "Assinante", action: "Voltar")
                    </div>
                    <div class="tabs-container">
                        <ul class="nav nav-tabs">
                            @if (ViewBag.Incluir == 0)
                            {
                                <li class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-2">Foto & Anexos</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-3">Endereços</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-4">Contatos</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-7">Observações</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-8">Pagamentos</a></li>
                            }
                            else
